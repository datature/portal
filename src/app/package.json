    "@uppy/core": "^1.13.2",
    "@uppy/dashboard": "^1.12.8",
    "apexcharts": "^3.37.1",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "electron-log": "^4.3.5",
    "env-cmd": "^10.1.0",
    "is-electron": "^2.2.0",
    "leaflet": "^1.6.0",
    "leaflet-draw": "^1.0.4",
