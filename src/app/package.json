{
  "name": "portal",
  "version": "0.5.5",
  "author": "Datature",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev": "next dev",
    "build": "NODE_ENV=production env-cmd -f environments/.env.build.local next build",
    "build:github": "next build",
    "build:static": "next build && next export -o ../../portal_build/out",
    "start": "env-cmd -f environments/.env.build.local next start",
    "lint": "eslint . --ext .js,.jsx,.tsx; exit 0",
    "preflight": "eslint . --config .eslintrc --ext .js,.jsx,.ts,.tsx; npx tsc --project tsconfig.json; npx prettier --check ./src/api/**/* ./src/components/**/* ./src/utils/**/* ./pages/**/*; next build; exit 0",
    "github-action:eslint": "npx eslint . --config .eslintrc  --ext .js,.jsx,.tsx",
    "github-action:prettier": "npx prettier --check ./src/api/**/* ./src/components/**/* ./src/utils/**/* ./pages/**/*"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
